int missingNumber(vector<int>& array, int n) {
         
        int count =0;
        sort(array.begin(),array.end());
        
        for(int i=0;i<n;i++)
        {
            count++;
            if(count!=array[i] )
            {
                
                break;
            }
            
        }
        // Your code goes here
        return count;
    
    }
