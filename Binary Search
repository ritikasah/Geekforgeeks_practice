int solve(int arr[],int start,int end,int key){
        int mid=start+(end-start)/2;
        if(start>end){
            return -1;
        }
        else if(arr[mid]==key){
            return mid;
        }
        else if (arr[mid]<key){
            return solve(arr,mid+1,end,key);
        }
        else{
            return solve(arr,start,mid-1,key);
        }
    }
    int binarysearch(int arr[], int n, int k) {
        int ans=solve(arr,0,n-1,k);
    
    }
